<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee	  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    <servlet>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>Servlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>GetGoods</servlet-name>
        <servlet-class>servlet.getGoods</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>GetGood</servlet-name>
        <servlet-class>servlet.getGood</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>AddGood</servlet-name>
        <servlet-class>servlet.addGood</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>GetCategory</servlet-name>
        <servlet-class>servlet.getCategory</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>servlet.home</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>Category</servlet-name>
        <servlet-class>servlet.getGoods</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>LogOut</servlet-name>
        <servlet-class>servlet.logOut</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>servlet.registration</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>Admin</servlet-name>
        <servlet-class>servlet.admin</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>Panel</servlet-name>
        <servlet-class>servlet.panel</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>addGood</servlet-name>
        <servlet-class>servlet.addGood</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>



    <servlet-mapping>
        <servlet-name>addGood</servlet-name>
        <url-pattern>/addGood</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Panel</servlet-name>
        <url-pattern>/panel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Admin</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOut</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Category</servlet-name>
        <url-pattern>/category</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetGoods</servlet-name>
        <url-pattern>/getGoods</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetGood</servlet-name>
        <url-pattern>/good</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddGood</servlet-name>
        <url-pattern>/addgood</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/s</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetCategory</servlet-name>
        <url-pattern>/getCategory</url-pattern>
    </servlet-mapping>
    <listener>
        <listener-class>tables.MyLifeCycleListener</listener-class>
    </listener>
</web-app>
